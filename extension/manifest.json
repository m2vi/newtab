{
  "manifest_version": 3,
  "name": "Newtab Extension",
  "version": "0.0.0",

  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png"
    },
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },

  "description": "Provides information to the Newtab page",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "background": {
    "service_worker": "contentScript.js"
  },

  "author": "m2v",
  "permissions": ["bookmarks"],
  "content_scripts": [
    {
      "matches": ["http://localhost:3000/*"],
      "js": ["contentScript.js"]
    }
  ]
}
